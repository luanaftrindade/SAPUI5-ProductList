<mvc:View
controllerName="sap.ui.demo/walkthrough.controllers.ProductList"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc">
    <Page>
    <content>
<List items="{products>/value}" headerText="Product List">
    <items>
        <StandardListItem 
            title="{products>ProductName}"
            description="{products>QuantityPerUnit}"
        />
    </items>
</List>

    <Button text="Details" press=".onShowProductDetails"/>
</content>

</Page>
</mvc:View>