<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core">
	<VBox class="sapUiSmallMargin">
		<f:SimpleForm id="Supplier"
			editable="false"
			layout="ResponsiveGridLayout"
			title="Supplier Details"
			labelSpanXL="4"
			labelSpanL="3"
			labelSpanM="4"
			labelSpanS="12"
			adjustLabelSpan="true"
			emptySpanXL="0"
			emptySpanL="4"
			emptySpanM="0"
			emptySpanS="0"
			columnsXL="2"
			columnsL="1"
			columnsM="1"
			singleContainerFullSize="false" >
			<f:content>
				<Label text="Company Name" />
				<Text text="{suppliers>CompanyName}" />
				<Label text="Contact Name" />
				<Text text="{suppliers>ContactName}" />
				<Label text="Contact Title" />
				<Text text="{suppliers>ContactTitle}" />
				<Label text="Address" />
				<Text  text="{suppliers>Address}" />
				<Label text="City" />
				<Text text="{suppliers>City}" />
				<Label text="Region" />
				<Text text="{suppliers>Region}" />
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>